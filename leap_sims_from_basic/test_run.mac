# Macro file for leap_sims
#
/leap_sims/phys/addPhysics polarized

# Initialize kernel
/run/initialize


/polarization/manager/verbose 1
/polarization/volume/set Magnet 0. 0. 1.
/polarization/volume/list
#
# Default kinematics:
# electron 60 MeV in direction (0.,0.,1.)
/gun/particle gamma
/gun/energy 3 MeV
/gun/polarization 0. 0. 1.

#/tracking/verbose 0
/run/printProgress 100000
/run/beamOn 1000000
